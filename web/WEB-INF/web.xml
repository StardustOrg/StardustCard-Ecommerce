<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.access.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>controller.access.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>controller.admin.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientHomeServlet</servlet-name>
        <servlet-class>controller.client.ClientHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArtistPageServlet</servlet-name>
        <servlet-class>controller.artist.ArtistPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>controller.access.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>controller.client.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <servlet-class>controller.client.DeleteAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllArtistsServlet</servlet-name>
        <servlet-class>controller.artist.AllArtistsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhotocardsServlet</servlet-name>
        <servlet-class>controller.admin.PhotocardsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArtistsServlet</servlet-name>
        <servlet-class>controller.admin.ArtistsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminUserServlet</servlet-name>
        <servlet-class>controller.admin.AdminUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teste</servlet-name>
        <servlet-class>controller.admin.Teste</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/Admin/Dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientHomeServlet</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ArtistPageServlet</servlet-name>
        <url-pattern>/Artists/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/Profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccountServlet</servlet-name>
        <url-pattern>/DeleteAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllArtistsServlet</servlet-name>
        <url-pattern>/Artists</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhotocardsServlet</servlet-name>
        <url-pattern>/Admin/Photocards</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ArtistsServlet</servlet-name>
        <url-pattern>/Admin/Artists</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SellingsServlet</servlet-name>
        <url-pattern>/Admin/Sellings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminUserServlet</servlet-name>
        <url-pattern>/Admin/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teste</servlet-name>
        <url-pattern>/Teste</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
</web-app>
